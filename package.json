{
  "name": "estudio-nexora-comet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run --reporter cypress-mochawesome-reporter",
    "test:e2e:full": "cypress run --reporter cypress-mochawesome-reporter --spec 'cypress/e2e/**/*.cy.ts'",
    "test:analytics": "cypress run --spec 'cypress/e2e/analytics.cy.ts'",
    "test:seo": "cypress run --spec 'cypress/e2e/seo-accessibility.cy.ts'",
    "test:mobile": "cypress run --spec 'cypress/e2e/mobile-responsive.cy.ts'",
    "test:errors": "cypress run --spec 'cypress/e2e/error-tracking.cy.ts'",
    "test:ai-blocking": "cypress run --spec 'cypress/e2e/ai-blocking-detection.cy.ts'",
    "test:ai-flow": "cypress run --spec 'cypress/e2e/ai-generation-flow.cy.ts'",
    "test:bilingual": "cypress run --spec 'cypress/e2e/bilingual-flow.cy.ts'",
    "test:ci": "npm run build && npm start & sleep 10 && cypress run --reporter cypress-mochawesome-reporter",
    "test:report": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json && npx marge cypress/reports/merged-report.json --reportDir cypress/reports/html",
    "export:csv": "node scripts/export-csv.js",
    "export:pdf": "node scripts/export-pdf.js",
    "qa:run": "npm run test:e2e:full && npm run test:report",
    "backup:supabase": "node scripts/backupSupabase.js",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "seo:audit": "node scripts/seo-lighthouse.js",
    "seo:audit:local": "node scripts/seo-lighthouse.js http://localhost:3000"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.0.0",
    "@google/generative-ai": "^0.2.0",
    "@notionhq/client": "^5.3.0",
    "@sanity/client": "^7.12.1",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/three": "^0.181.0",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.5.0",
    "stripe": "^14.10.0",
    "three": "^0.161.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@sentry/browser": "^10.23.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "cypress": "^15.6.0",
    "cypress-axe": "^1.7.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
